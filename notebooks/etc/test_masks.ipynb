   "source": [
    "from hyperscope import config\n",
    "\n",
    "unique_values = set()\n",
    "for path in (config.INTERIM_DATA_DIR / 'worms' / 'masks').iterdir():\n",
    "    if path.suffix != '.npz': continue\n",
    "    a = np.load(str(path))['arr_0']\n",
    "    for u in np.unique(a):\n",
    "        unique_values.add(u.item())\n",
    "\n",
    "unique_values"
   ]
  }
 ],
